[Desktop Entry]
Version=1.0
_Name=Orage Calendar
_Comment=Desktop calendar
Exec=orage %F
Icon=org.xfce.orage
StartupNotify=true
MimeType=text/calendar;
Terminal=false
Type=Application
Categories=X-XFCE;Office;Calendar;GTK;
_GenericName=Calendar
Actions=Today;NewAppointment;Preferences;

[Desktop Action Today]
_Name=Show Today
Exec=orage --today

[Desktop Action NewAppointment]
_Name=New Appointment
Exec=orage --new-appointment

[Desktop Action Preferences]
_Name=Orage Preferences
Exec=orage --preferences